{
  "name": "news_reader",
  "version": "1.0.0",
  "description": "news reader",
  "main": "index.js",
  "scripts": {
    "test": "node --harmony_proxies node_modules/.bin/jest",
    "test:watch": "npm test -- --watch",
    "start:server": "nodemon ./server/server.js --exec babel-node --presets es2015,stage-2",
    "start:client": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 5000 --config webpack.config.devserver.js",
    "start:prod": "node server.js",
    "prebuild:min": "rm -rf dist",
    "build:client": "webpack client/src/index.js --config webpack.config.production.js --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbyonivo/news_reader.git"
  },
  "author": "ivo orkuma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gbyonivo/news_reader/issues"
  },
  "jest": {
    "bail": true,
    "notify": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/src/**/*.js"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    },
    "verbose": false,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|webm)$": "<rootDir>/jestFileTransformer.js"
    }
  },
  "homepage": "https://github.com/gbyonivo/news_reader#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "superagent": "^3.8.1",
    "superagent-xml2jsparser": "^0.1.1",
    "xml-js": "^1.5.2",
    "xml-json": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "cors": "^2.8.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.13.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "http-server": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.2.0",
    "nodemon": "^1.12.1",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^16.1.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
